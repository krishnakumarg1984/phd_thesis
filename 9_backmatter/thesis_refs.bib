keywords = {Battery management system,Electrochemical modeling,Knot placement,Lithium-ion battery,Model parameterization,Piecewise linearization},
pages = {351--362},
publisher = {Elsevier B.V},
title = {{Continuous piecewise-linear, reduced-order electrochemical model for lithium-ion batteries in real-time applications}},
url = {http://dx.doi.org/10.1016/j.jpowsour.2016.12.044},
volume = {342},
year = {2017}
}
@article{Kemper2015,
abstract = {A sophisticated BMS often requires an accurate yet simple battery model. Simplified models such as the single particle model (SPM), however, provide limited insight because lithium concentration variations over the cell thickness dimension are often neglected. In this study, we propose a simplified electrochemical model that, by reviving the lithium concentration variation across the cell thickness, provides significantly improved prediction power over the SPM. The lithium concentration profiles are first derived according to the electrochemistry-based pseudo-two dimensions (P2D) model under the steady-state assumption, they are then relaxed into a dynamic model. By employing steady-state concentration profiles coupled with averaged dynamics, the proposed model predicts electrolyte and solid surface concentrations (ce and css, respectively) and potential variations across the cell during both transients and stead-state conditions. The proposed model is validated by comparing the cell voltage, ce, and css concentration RMS errors with respect to the P2D model under pulse and constant current inputs. The simulation shows that the proposed model achieves at least 74{\%}, 77{\%}, and 65{\%} RMS error reductions for cell voltage, css and ce, respectively, when compared to SPM in constant current simulation. Finally, the presented model is used to predict and detect local lithium plating, which the SPM is not capable of doing; our method yields predictions similar to those of the original P2D model at computational loads comparable to SPM.},
author = {Kemper, Paulo and Li, Shengbo Eben and Kum, Dongsuk},
doi = {10.1016/J.JPOWSOUR.2015.03.134},
file = {:home/kg314/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Kemper, Li, Kum - 2015 - Simplification of pseudo two dimensional battery model using dynamic profile of lithium concentration.pdf:pdf},
issn = {0378-7753},
journal = {Journal of Power Sources},
month = {jul},
pages = {510--525},
publisher = {Elsevier},
title = {{Simplification of pseudo two dimensional battery model using dynamic profile of lithium concentration}},
url = {https://www.sciencedirect.com/science/article/pii/S0378775315005583},
volume = {286},
year = {2015}
}
