file = {:home/kg314/Documents/mendeley{\_}desktop/2015/Journal of Power Sources/Guo, Li, Pecht/Guo, Li, Pecht{\_}A Bayesian approach for Li-Ion battery capacity fade modeling and cycles to failure prognostics{\_}2015.pdf:pdf},
