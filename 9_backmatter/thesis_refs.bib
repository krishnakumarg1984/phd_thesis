file = {:home/kg314/Documents/mendeley{\_}desktop/2017/Journal of Power Sources/Farag, Fleckenstein, Habibi/Farag, Fleckenstein, Habibi{\_}Continuous piecewise-linear, reduced-order electrochemical model for lithium-ion batteries in real-time appl.pdf:pdf},
