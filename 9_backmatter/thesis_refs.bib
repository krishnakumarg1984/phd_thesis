journal = {Journal of Power Sources},
keywords = {Calendaric,Cycle,Effect,Electrode,Lithiumironphosphate,Passive},
pages = {254--263},
title = {{Systematic aging of commercial LiFePO4|Graphite cylindrical cells including a theory explaining rise of capacity during aging}},
volume = {345},
year = {2017}
}
