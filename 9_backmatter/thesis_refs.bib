keywords = {DSCC2015-9693},
pages = {1--9},
publisher = {American Society of Mechanical Engineers},
title = {{Estimation of Lithium-Ion Concentrations in Both Electrodes of a Lithium-Ion Battery Cell}},
url = {http://proceedings.asmedigitalcollection.asme.org/proceeding.aspx?articleid=2481947},
year = {2015}
