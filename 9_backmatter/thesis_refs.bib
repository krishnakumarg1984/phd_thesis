file = {:home/kg314/Documents/mendeley{\_}desktop/2013/Journal of Power Sources/Marcicki et al/Marcicki et al.{\_}Design and parametrization analysis of a reduced-order electrochemical model of graphiteLiFePO4 cells for SOCSOH estimat.pdf:pdf},
