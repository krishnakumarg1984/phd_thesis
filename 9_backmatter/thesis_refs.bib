keywords = {Battery management system,Electrochemical modeling,Knot placement,Lithium-ion battery,Model parameterization,Piecewise linearization},
pages = {351--362},
publisher = {Elsevier B.V},
title = {{Continuous piecewise-linear, reduced-order electrochemical model for lithium-ion batteries in real-time applications}},
url = {http://dx.doi.org/10.1016/j.jpowsour.2016.12.044},
volume = {342},
year = {2017}
}
