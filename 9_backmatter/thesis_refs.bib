doi = {10.1109/CDC.2016.7799133},
file = {:home/kg314/Documents/mendeley{\_}desktop/2016/2016 IEEE 55th Conference on Decision and Control, CDC 2016/Ascencio, Astolfi, Parisini/Ascencio, Astolfi, Parisini{\_}Backstepping PDE-based adaptive observer for a Single Particle Model of Lithium-Ion Batteries{\_}2016.pdf:pdf},
isbn = {9781509018376},
journal = {55th Conference on Decision and Control},
number = {Cdc},
