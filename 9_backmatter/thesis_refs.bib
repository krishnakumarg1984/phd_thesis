pages = {262--276},
publisher = {Elsevier},
title = {{Extended Kalman filtering for battery management systems of LiPB-based HEV battery packs: Part 2. Modeling and identification}},
url = {https://www.sciencedirect.com/science/article/pii/S037877530400360X},
volume = {134},
year = {2004}
}
@article{Wang2017,
