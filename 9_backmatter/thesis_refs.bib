issn = {0378-7753},
journal = {Journal of Power Sources},
month = {nov},
pages = {187--201},
publisher = {Elsevier},
title = {{A physics-based fractional order model and state of energy estimation for lithium ion batteries. Part I: Model development and observability analysis}},
url = {https://www.sciencedirect.com/science/article/pii/S0378775317312570},
volume = {367},
year = {2017}
}
