file = {:home/kg314/Documents/mendeley{\_}desktop/2007/Journal of Power Sources/Zhang, White/Zhang, White{\_}Comparison of approximate solution methods for the solid phase diffusion equation in a porous electrode model{\_}2007.pdf:pdf},
issn = {0378-7753},
journal = {Journal of Power Sources},
month = {mar},
number = {2},
pages = {880--886},
publisher = {Elsevier},
title = {{Comparison of approximate solution methods for the solid phase diffusion equation in a porous electrode model}},
