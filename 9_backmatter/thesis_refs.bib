keywords = {Incremental capacity analysis,Lithium deposition,Lithium-ion battery,Low-temperature aging,Low-temperature charging},
pages = {309--320},
publisher = {Elsevier B.V},
title = {{Low temperature aging mechanism identification and lithium deposition in a large format lithium iron phosphate battery for different charge profiles}},
