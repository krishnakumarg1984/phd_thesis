doi = {10.1016/J.JPOWSOUR.2017.01.126},
file = {:home/kg314/Documents/mendeley{\_}desktop/2017/Journal of Power Sources/Zhang et al/Zhang et al.{\_}A novel method for identification of lithium-ion battery equivalent circuit model parameters considering electrochemical pr.pdf:pdf},
