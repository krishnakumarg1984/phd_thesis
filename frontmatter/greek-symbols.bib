% Encoding: UTF-8

@symbol{eps,
    name={\ensuremath{\varepsilon}},
    description={Electrode porosity \ie{} volume fraction of electrolyte phase within the electrode }
}

@symbol{epssolid,
    name={\ensuremath{\varepsilon_\mathrm{s}}},
    description={Solid phase volume fraction}
}

@symbol{epsfi,
    name={\ensuremath{\varepsilon_\mathrm{fi}}},
    description={Filler/binder volume fraction}
}

@symbol{kappa,
    name={\ensuremath{\kappa}},
    description={Intrinsic ionic conductivity of the electrolyte~[\si{\siemens\per\meter}]}
}

@symbol{Keff,
    name={\ensuremath{\kappa_\mathrm{eff}}},
    description={Effective ionic conductivity of the electrolyte~[\si{\siemens\per\meter}]}
}

@symbol{phie,
    name={\ensuremath{\phi_\mathrm{e}}},
    description={Electric potential in the electrolyte phase~[\si{\volt}]}
}

@symbol{phis,
    name={\ensuremath{\phi_\mathrm{s}}},
    description={Electric potential of the electrode~[\si{\volt}]}
}

@symbol{sigmaeff,
    name={\ensuremath{\sigma_\mathrm{eff}}},
    description={Effective electronic conductivity of the electrode~[\si{\siemens\per\meter}]}
}

@symbol{sigma,
    name={\ensuremath{\sigma}},
    description={Intrinsic electronic conductivity of the electrode~[\si{\siemens\per\meter}]}
}

@symbol{omega,
    name={\ensuremath{\Omega}},
    description={Control volume in a standard finite-volume discretisation scheme}
}

@symbol{eta,
    name={\ensuremath{\eta}},
    description={Electrode overpotential~[\si{\volt}]}
}

@symbol{rhoair,
    name={\ensuremath{\rho_\mathrm{air}}},
    description={Air density at acceleration test conditions~[\si{\kilogram\per\meter\cubed}]}
}

@symbol{etadt,
    name={\ensuremath{\eta_\mathrm{dt}}},
    description={Overall lumped efficiency of the drivetrain components (excluding battery pack) of a (hybrid) electric vehicle}
}

@symbol{rho,
    name={\ensuremath{\rho_\lambda}},
    description={Density of each constitutent component~$\lambda \in \{\mathrm{neg,sep,pos,Al,Cu,pouch,\ch{LiPF_6}}\}$ of the cell~[\si{\kilogram\per\meter\cubed}]}
}

