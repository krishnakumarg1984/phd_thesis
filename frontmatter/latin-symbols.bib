@symbol{brugg,
    name={\ensuremath{\mathrm{brugg}}},
    description={Bruggeman coefficient (empirical) to account for the tortuosity of a medium}
}

@symbol{cs,
    name={\ensuremath{c_\mathrm{s}}},
    description={Concentration of \ch{Li^0} in the solid phase \ie{} electrode (\si{\mol\per\meter\cubed})}
}

@symbol{Ds,
    name={\ensuremath{D_\mathrm{s}}},
    description={Intrinsic diffusivity of the electrode material (\si{\meter\squared\per\second})}
}

@symbol{Dseff,
