% Encoding: UTF-8

@symbol{cs,
    name={\ensuremath{c_\mathrm{s}}},
    description={Concentration of \ch{Li^0} in the solid phase \ie{} electrode (\si{\mol\per\meter\cubed})}
}

@symbol{Ds,
    name={\ensuremath{D_\mathrm{s}}},
    description={Diffusivity of the electrode material (\si{\meter\squared\per\second})}
}

@symbol{r,
    name={\ensuremath{r}},
    description={Radial dimension \ie{} the continuous co-ordinate along the particle radius where solid phase concentration is to be computed (\si{\meter})}
}

@symbol{Rp,
    name={\ensuremath{R_p}},
    description={Representative particle radius of the solid phase as used in the model (\si{\meter})}
}

@symbol{j,
    name={\ensuremath{j}},
    description={Molar flux density at particle surface \ie{} electrode-electrolyte interface (\si{\mol\per\meter\squared\per\second})}
}

@symbol{ce,
    name={\ensuremath{c_\mathrm{e}}},
    description={Concentration of \ch{Li^+} ions in the electrolyte phase (\si{\mol\per\meter\cubed})}
}

@symbol{t,
    name={\ensuremath{t}},
    description={time (\si{\second})}
}

@symbol{x,
    name={\ensuremath{x}},
    description={Axial dimension \ie{} the continuous co-ordinate along the through-thickness direction of the cell (\si{\meter})}
}

@symbol{Deff,
    name={\ensuremath{D_\mathrm{eff}\enspace}},
    description={Effective ionic diffusivity of the electrolyte (\si{\meter\squared\per\second})}
}

@symbol{tplus,
    name={\ensuremath{{t^0_\text{+}}}},
    description={Transference number of \ch{Li^+} ions with respect to the solvent}
}

@symbol{as,
    name={\ensuremath{a_s}},
    description={Specific interfacial surface area of the electrode (\si{\meter\squared\per\meter\cubed})}
}

@symbol{ltot,
    name={\ensuremath{l_\mathrm{tot}}},
    description={Combined thickness of negative electrode, separator and the positive electrode (\si{\meter})}
}

@symbol{F,
    name={\ensuremath{F}},
    description={Faraday constant (\si{\coulomb\per\mol})}
}


@symbol{R,
    name={\ensuremath{R}},
    description={Universal gas constant (\si{\joule\per\mol\per\kelvin})}
}


@symbol{T,
    name={\ensuremath{T}},
    description={Lumped cell temperature (\si{\kelvin})}
}

