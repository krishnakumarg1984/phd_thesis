% Encoding: UTF-8

@symbol{neg,
    name={\ensuremath{\mathrm{neg}}},
    description={Denotes the negative electrode region. Alternatively, a subscripted `$n$' is also used.}
}

@symbol{sep,
    name={\ensuremath{\mathrm{sep}}},
    description={Denotes the separator region.  Alternatively, a subscripted `$s$' is also used.}
}

@symbol{pos,
    name={\ensuremath{\mathrm{pos}}},
    description={Denotes the positive electrode region. Alternatively, a subscripted `$p$' is also used.}
}

@symbol{negsep,
    name={\ensuremath{\mathrm{neg/sep}}},
    description={Denotes the negative electrode/separator interface}
}

@symbol{seppos,
    name={\ensuremath{\mathrm{sep/pos}}},
    description={Denotes the positive electrode/separator interface}
}

@symbol{negcucc,
    name={\ensuremath{\mathrm{neg/Cucc}\ \ }},
    description={Denotes the negative electrode/Copper current collector interface}
}

@symbol{posalcc,
    name={\ensuremath{\mathrm{pos/Alcc}}},
    description={Denotes the positive electrode/Aluminium current collector interface}
}

